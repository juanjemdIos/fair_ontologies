	dcterms:title "Ontology prefix is found in prefix.cc or LOV"@en ;
	rdfs:label "FIND2 test: Ontology prefix is found in prefix.cc or LOV"^^xsd:string ;
	vivo:abbreviation "FIND2-T"^^xsd:string ;
